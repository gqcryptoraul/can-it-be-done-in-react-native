{
  "name": "sticker-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "tsc": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx  --max-warnings 0",
